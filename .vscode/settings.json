{
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "handlebars"],
    "eslint.options": {
        "extensions": [".mjs", ".js", ".jsx", ".ts", ".tsx"]
    },
    "files.associations": {
        "*.mjs": "javascript",
        "*.hbs": "handlebars"
    },
    "prettier.requireConfig": true
}
