    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "plugins": ["prettier-plugin-hbs"],
    "overrides": [
        {
            "files": "**/*.hbs",
            "options": {
                "parser": "html"
            }
        }
    ]
}
